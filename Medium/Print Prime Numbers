SELECT LISTAGG(L,'&') WITHIN GROUP (ORDER BY L)
FROM
(
SELECT LEVEL L FROM DUAL CONNECT BY LEVEL <= 1000
)
WHERE L <> 1 AND L NOT IN
(
SELECT L1 FROM
(SELECT LEVEL L1 FROM DUAL CONNECT BY LEVEL <= 1000) L1,
(SELECT LEVEL L2 FROM DUAL CONNECT BY LEVEL < 1000) L2
WHERE L2 < L1 AND MOD(L1,L2) = 0 AND L2 <> 1
);
